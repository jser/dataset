{
  "name": "@jser/stat",
  "version": "1.0.2",
  "description": "JSer.info stat library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "data/*.js",
    "lib",
    "src"
  ],
  "scripts": {
    "prepublish": "npm run --if-present build",
    "build": "cross-env NODE_ENV=production tsc -p .",
    "watch": "tsc -p . --watch",
    "test": "mocha 'test/**/*.js'",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,css}'",
    "precommit": "lint-staged",
    "postcommit": "git reset",
    "clean": "rimraf lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jser/stat-js.git"
  },
  "keywords": [
    "jser"
  ],
  "author": "azu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jser/stat-js/issues"
  },
  "homepage": "https://github.com/jser/stat-js",
  "devDependencies": {
    "@jser/data-fetcher": "^1.0.1",
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.46",
    "cross-env": "^5.1.0",
    "lint-staged": "^7.0.5",
    "mocha": "^5.1.1",
    "power-assert": "^1.4.2",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.2",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "array.prototype.find": "^2.0.1",
    "binarysearch": "^1.0.0",
    "lodash.sortby": "^4.7.0",
    "natural": "^0.5.6",
    "request": "^2.57.0"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}

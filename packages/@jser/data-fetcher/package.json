{
  "name": "@jser/data-fetcher",
  "version": "1.1.3",
  "description": "Data Fetcher library for dataset.",
  "keywords": [
    "data",
    "jser"
  ],
  "homepage": "https://github.com/jser/dataset/tree/master/packages/@jser/data-fetcher/",
  "bugs": {

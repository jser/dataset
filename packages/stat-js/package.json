  "devDependencies": {
    "babel": "^5.5.8",
    "esdoc": "^0.1.2",
    "espower-babel": "^3.2.0",
    "gh-pages": "^0.3.1",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0"
  },
  "dependencies": {
    "request": "^2.57.0"
  }
